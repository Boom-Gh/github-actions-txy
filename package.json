    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.4",
    "@types/node": "^20.6.3",
    "@types/nprogress": "^0.2.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.49.0",
